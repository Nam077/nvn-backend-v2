import { ApiProperty } from '@nestjs/swagger';
import { IsNotEmpty, IsString } from 'class-validator';

export class Create<%= modelName %>Dto {
    @ApiProperty({
        description: 'The name of the <%= modelNameSingularLowerCase %>.',
        example: 'Example <%= modelName %>',
    })
    @IsString()
    @IsNotEmpty()
    name: string;
} 